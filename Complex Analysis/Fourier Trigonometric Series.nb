   "\[InvisibleSpace]", 
   RowBox[{"-", 
    FractionBox[
     RowBox[{"16", " ", 
      RowBox[{"Sin", "[", 
       FractionBox[
        RowBox[{"n", " ", "\[Pi]", " ", "t"}], "2"], "]"}]}], 
     RowBox[{"n", " ", "\[Pi]"}]]}], "\[InvisibleSpace]", "\<\"))\"\>"}],
  SequenceForm["f(t)=", 
   Rational[16, 3], 
   "+\!\(\*UnderoverscriptBox[\(\[Sum]\), \(n = 1\), \(\[Infinity]\)]\)(", 
   16 $CellContext`n^(-2) Pi^(-2) 
