Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"CompF", "[", 
    RowBox[{"n_", ",", "t_"}], "]"}], "=", 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"k", "=", 
      RowBox[{"-", "n"}]}], "n"], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"cn", "[", "k", "]"}], "*", 
      SuperscriptBox["E", 
       RowBox[{
        RowBox[{"-", "I"}], "*", "k", "*", "\[Omega]", "*", "t"}]]}], 
     ")"}]}]}], ";"}], "\n", 
 RowBox[{"Simplify", "[", 
  RowBox[{"ExpToTrig", "[", 
   RowBox[{"CompF", "[", 
    RowBox[{"2", ",", "t"}], "]"}], "]"}], "]"}]}], "Code",
