Cell[BoxData[
 InterpretationBox[
  RowBox[{"\<\"The complex Fourier coefficient is given by \
\\!\\(\\*SubscriptBox[\\(c\\), \\(n\\)]\\)=\"\>", "\[InvisibleSpace]", 
   TagBox[GridBox[{
      {"\[Piecewise]", GridBox[{
         {
          FractionBox["16", "3"], 
          RowBox[{"n", "\[Equal]", "0"}]},
         {
          RowBox[{
           FractionBox["1", "2"], " ", 
           RowBox[{"(", 
            RowBox[{
             FractionBox["16", 
              RowBox[{
               SuperscriptBox["n", "2"], " ", 
               SuperscriptBox["\[Pi]", "2"]}]], "+", 
             FractionBox[
              RowBox[{"16", " ", "i"}], 
              RowBox[{"n", " ", "\[Pi]"}]]}], ")"}]}], 
          RowBox[{"n", ">", "0"}]},
         {
          RowBox[{
           FractionBox["1", "2"], " ", 
           RowBox[{"(", 
            RowBox[{
             FractionBox["16", 
              RowBox[{
               SuperscriptBox["n", "2"], " ", 
               SuperscriptBox["\[Pi]", "2"]}]], "-", 
             FractionBox[
              RowBox[{"16", " ", "i"}], 
              RowBox[{"n", " ", "\[Pi]"}]]}], ")"}]}], 
          RowBox[{"n", "<", "0"}]},
         {"0", 
          TagBox["True",
           "PiecewiseDefault",
           AutoDelete->True]}
        },
        AllowedDimensions->{2, Automatic},
        Editable->True,
        GridBoxAlignment->{"Columns" -> {{Left}}, "Rows" -> {{Baseline}}},
        GridBoxItemSize->{"Columns" -> {{Automatic}}, "Rows" -> {{1.}}},
        GridBoxSpacings->{"Columns" -> {
            Offset[0.27999999999999997`], {
             Offset[0.84]}, 
            Offset[0.27999999999999997`]}, "Rows" -> {
            Offset[0.2], {
             Offset[0.4]}, 
            Offset[0.2]}},
        Selectable->True]}
     },
     GridBoxAlignment->{"Columns" -> {{Left}}, "Rows" -> {{Baseline}}},
     GridBoxItemSize->{"Columns" -> {{Automatic}}, "Rows" -> {{1.}}},
     GridBoxSpacings->{"Columns" -> {
         Offset[0.27999999999999997`], {
          Offset[0.35]}, 
         Offset[0.27999999999999997`]}, "Rows" -> {
         Offset[0.2], {
          Offset[0.4]}, 
         Offset[0.2]}}],
    "Piecewise",
    DeleteWithContents->True,
    Editable->False,
    SelectWithContents->True,
    Selectable->False,
    StripWrapperBoxes->True]}],
  SequenceForm[
  "The complex Fourier coefficient is given by \!\(\*SubscriptBox[\(c\), \
\(n\)]\)=", 
   Piecewise[{{
      Rational[16, 3], $CellContext`n == 0}, {
     Rational[1, 2] (16 $CellContext`n^(-2) Pi^(-2) + 
       16 $CellContext`i $CellContext`n^(-1)/Pi), $CellContext`n > 0}, {
