a619b44a59ca"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"Print", "[", 
  RowBox[{"\"\<Then, the Fourier Series expansion of f(t)=\>\"", ",", " ", 
   RowBox[{"f", "[", "t", "]"}], ",", " ", "\"\< is given by: \>\""}], 
  "]"}], "\n", 
 RowBox[{"Print", "[", 
  RowBox[{"\"\<f(t)=\>\"", ",", 
   FractionBox[
    RowBox[{"a0", "[", "n", "]"}], "2"], ",", 
   "\"\<+\!\(\*UnderoverscriptBox[\(\[Sum]\), \(n = 1\), \
\(\[Infinity]\)]\)(\>\"", ",", " ", 
   RowBox[{
    RowBox[{"an", "[", "n", "]"}], "*", 
    RowBox[{"Cos", "[", 
     RowBox[{"n", "*", "\[Omega]", "*", "t"}], "]"}]}], ",", "\"\<)+(\>\"", 
   ",", " ", 
   RowBox[{
    RowBox[{"bn", "[", "n", "]"}], "*", 
    RowBox[{"Sin", "[", 
     RowBox[{"n", "*", "\[Omega]", "*", "t"}], "]"}]}], ",", "\"\<))\>\""}], 
  "]"}]}], "Code",
