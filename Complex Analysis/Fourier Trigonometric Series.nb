Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"cn", "[", "n", "]"}], "=", 
   RowBox[{"Piecewise", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"a0", "[", "n", "]"}], "/", "2"}], ",", 
        RowBox[{"n", "==", "0"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Refine", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"an", "[", "n", "]"}], "-", 
             RowBox[{"(", 
              RowBox[{"i", "*", 
               RowBox[{"bn", "[", "n", "]"}]}], ")"}]}], ")"}], "/", "2"}], 
          ",", 
          RowBox[{"Element", "[", 
           RowBox[{"n", ",", "Integers"}], "]"}]}], "]"}], ",", 
        RowBox[{"n", ">", "0"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Refine", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"an", "[", "n", "]"}], "+", 
             RowBox[{"(", 
              RowBox[{"i", "*", 
               RowBox[{"bn", "[", "n", "]"}]}], ")"}]}], ")"}], "/", "2"}], 
          ",", 
          RowBox[{"Element", "[", 
           RowBox[{"n", ",", "Integers"}], "]"}]}], "]"}], ",", 
        RowBox[{"n", "<", "0"}]}], "}"}]}], "}"}], "]"}]}], ";"}], "\n", 
 RowBox[{"Print", "[", 
  RowBox[{
  "\"\<The complex Fourier coefficient is given by \!\(\*SubscriptBox[\(c\), \
\(n\)]\)=\>\"", ",", " ", 
   RowBox[{"cn", "[", "n", "]"}]}], "]"}]}], "Code",
