    RowBox[{"\[Pi]", "/", "T"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{"\"\<The functions is f(x)=\>\"", ",", " ", 
    RowBox[{"f", "[", "x", "]"}], ",", " ", "\"\< with a period T=\>\"", ",", 
    " ", "T", ",", " ", "\"\< and a frequency \[Omega]=\>\"", ",", 
    "\[Omega]"}], "]"}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Plot", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"f", "[", "x", "]"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", 
      RowBox[{"(", 
       RowBox[{"a", "-", "1"}], ")"}], ",", 
      RowBox[{"(", 
       RowBox[{"b", "+", "1"}], ")"}]}], "}"}], ",", 
    RowBox[{"PlotLabel", "\[Rule]", "\"\<Function to expand\>\""}]}], "]"}], 
  "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"a0", "[", "n_", "]"}], "=", 
